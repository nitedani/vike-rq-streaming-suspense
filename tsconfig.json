{
  "compilerOptions": {
    "baseUrl": ".",
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strict": true,
    "strictNullChecks": true,
    "module": "esnext",
    "moduleResolution": "Node",
    "target": "esnext",
    "outDir": "dist",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "types": ["vite/client"],
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strictPropertyInitialization": false,
    "paths": {
      "#root/*": ["./*"]
    }
  }
}
